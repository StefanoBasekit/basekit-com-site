{% extends "layout.twig" %}
{% block templatehead %}
	<!-- Include responsive columns for default template type -->
	<link rel="stylesheet" href="//{{env.assetDomain}}/templates/common/responsive-columns.css" />

{% endblock %}

{% block content %}

<div class="bk-pt-default">

<!-- HEADER !-->
{% include '/includes/header.twig' %}

<!-- SUB NAV BAR !-->
<section class="partners-subnav">
    <div class="wrapper">
    	{{widget('content', 'subavheader', {"content":'<h2 class="com-header hwhite">The developer community is comingâ€¦</h2>'})|raw}}
        {{widget('content', 'communitystrapline', {"content":'<p class="com-strap">Create fully responsive templates using CSS, LESS, Github and other familiar technologies.</p>'})|raw}}
    </div>
</section>

<!-- DEVELOPER COMMUNITY - DESKTOP !-->
<section class="community-resources LightGrey">
    <div class="wrapper">
        {{widget('content', 'developercomminitycontent1', {"content":'<h2 class="h2-36pad text-center">Community Resources</h2>'})|raw}}
        <!-- DEVELOPER COMMUNITY - DESKTOP !-->
        <div class="community-resources LightGrey">
            <div class="res-col columns span-30-31-30">
                <div class="circle-graphics clearfix">
                    <div class="column span-three-left">
                        <img src="{{asset('/images/bk-docs-img.png')}}">
                        {{widget('content', 'community1', {"content":'<h3 class="line">Documentation</h3><p>Read the <a href="http://developers.basekit.com">online documentation</a> and start building your own template.</p>'})|raw}}
                    </div>
                    <div class="column span-three-middle">
                        <img src="{{asset('/images/bk-support-img.png')}}">
                        {{widget('content', 'community2', {"content":'<h3 class="line">Questions &amp; Answers</h3><p>Having trouble with your BaseKit website? Head over to our <a href="https://basekit.zendesk.com/home">Q&amp;A</a> section.</p>'})|raw}}
                    </div>
                    <div class="column span-three-right">
                        <img src="{{asset('/images/bk-work-img.png')}}">
                        {{widget('content', 'community3', {"content":'<h3 class="line">Workshop</h3><p>Get ready for all new workshops, coming soon.</p>'})|raw}}
                    </div>
                </div>
                <div class="feature-highlight text-center clearfix">
                    <h4>Plus <span>free</span> tailored developer accounts</h4>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- SIGNUP FORM !-->
<section class="register-form">
    <div class="wrapper clearfix">
        <div class="res-col columns span-54-46">
            <div class="column span-two-left">
                {{widget('content', 'registerheader', {"content":'<h2 class="h2-36pad">Register your interest</h2>'})|raw}}
                {{widget('content', 'registercontent', {"content":'<p>Interested in our developer community? Sign up today, stay informed and get the latest updates on everything BaseKit.</p>'})|raw}}
            </div>
            <div class="com column span-two-right LightGrey">
                {{widget('signupform', 'bk-signup', {'text': 'Send', 'label': 'Your Email', 'formTitle': 'Signup Form', 'email': 'profile', 'goalUrl': 'custom'})|raw}}
            </div>
        </div>
    </div>
</section>

<!-- PARTNER BAR !-->
<section class="partner-bar bottom LightGrey">
    <div class="wrapper">
        <!-- BAR - DESKTOP !-->
        <div class="desktop">
            <div class="res-col columns span-12-21x3-27">
                <div class="column span-five-first">
                    {{widget('content', 'partnerbar1content1', {"content":'<h4 class="h4light">Proud to<br> Partner With</h4>'})|raw}}
                </div>
                <div class="column span-five">
                    <img src="{{asset('/images/partner-logos/host-europe.png')}}">
                </div>
                <div class="column span-five">
                    <img src="{{asset('/images/partner-logos/123-reg.png')}}" class="reg-logo">
                </div>
                <div class="column span-five">
                    <img src="{{asset('/images/partner-logos/fasthosts.png')}}" class="enom-logo">
                </div>
                <div class="column span-five-last">
                    <a href="/partners" class="button main-btn green">Become a Partner</a>
                </div>
            </div>
        </div>
        <!-- BAR - TABLET !-->
        <div class="partner-res">
            {{widget('content', 'partnerbar2content1', {"content":'<h3><span class="mobHide">Start your free, <span class="darkspan">no-risk</span>, BaseKit trial!</span><a href="/partners" class="button main-btn green">Become a Partner</a></h3>'})|raw}}
        </div>
    </div>
</section>

</div>

<!-- FOOTER !-->
{% include '/includes/footer.twig' %}

{% endblock %}
