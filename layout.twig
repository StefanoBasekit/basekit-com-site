<!doctype html>
<!--[if lt IE 7]>
	<html class="no-js ie6 oldie" lang="en">
<![endif]-->
<!--[if IE 7]>
	<html class="no-js ie7 oldie" lang="en">
<![endif]-->
<!--[if IE 8]>
	<html class="no-js ie8 oldie" lang="en">
<![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js" lang="{{ page.seoLang }}">
<!--<![endif]-->
<head>
	<meta charset="utf-8" />
	<title>{{ page.title }}</title>
	<meta name="keywords" content="{{ page.keywords }}" />
	<meta name="description" content="{{ page.description }}" />
	<meta http-equiv="content-language" content="{{ page.seoLang }}" />
	<meta property="og:image" content="{{asset('/images/facebook-og-preview.jpg')}}">

	{% if site.favicon %}
		<link rel="icon" href="{{ site.favicon['favicon'] }}" type="image/x-icon" />
		<link rel="SHORTCUT ICON" href="{{ site.favicon['thumbnail'] }}?v={{ "now"|date("U") }}" type="image/x-icon" />
	{% endif %}
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<!-- <meta name="viewport" content="width=1024"> -->
	<meta name="viewport" content="width=device-width,initial-scale=1">

	<link rel="stylesheet" type="text/css" href="{{asset('/fonts/font-awesome/css/font-awesome.min.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/js/previewer/stylesheet.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/js/slider/css/unoSlider.min.css')}}" />
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>

	{% block templatehead %}
	{% endblock %}

	{% include basekit.headScript %}

</head>

<body class="{{ page.backgroundClass }}">

	{% block content %}
	{% endblock %}

	{% include basekit.bodyScript %}

	<!-- <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
	<script>window.jQuery || document.write('<script src="{{ asset('libs/jquery-1.10.2.min.js')}}"><\/script>')</script> -->

	<script src="{{asset('js/previewer/preview.js')}}"></script>
	<script src="{{asset('js/divStick.js')}}"></script>

	<script type="text/javascript">
	$(document).ready(function() {
	    $("#setup").click(function() {
	        $("#closeable").slideToggle(function() {
	            if ($(this).is(":visible")) {
	                $("#setup").text("View less designs");
	            }
	            else {
	                $("#setup").text("View more designs");
	            }
	        });
	    });
	});
	</script>

	<script type="text/javascript">
	$(document).ready(function() {
	    $("#hide-editor").click(function() {
	        $("#close").slideToggle(function() {
	            if ($(this).is(":visible")) {
	                $("#hide-editor").text("Hide editor features");
	            }
	            else {
	                $("#hide-editor").text("View editor features");
	            }
	        });
	    });
	});
	</script>

	<script type="text/javascript">
	    $(".pacman-feature").css("height", $(window).height());
	        $(window).bind("resize",function() {
	            $(".pacman-feature").css("height", $(window).height());
	        });
	</script>

	{% block templatebody %}
	{% endblock %}

</body>
</html>
