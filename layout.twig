	<link rel="stylesheet" type="text/css" href="{{asset('/fonts/font-awesome/css/font-awesome.min.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/js/previewer/stylesheet.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/js/slider/css/unoSlider.min.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/js/responsiveNav/resNav.css')}}" />

	<!-- <link rel="stylesheet" type="text/css" href="{{asset('/js/navScroll/css/demo.css')}}" /> -->

	{% if page.templateType == "default" %}
	<!-- Include responsive columns for default template type -->
	<link rel="stylesheet" href="//{{env.assetDomain}}/templates/common/responsive-columns.css" />
