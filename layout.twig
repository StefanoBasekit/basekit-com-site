	<script src="{{asset('js/previewer/preview.js')}}"></script>
	<script src="{{asset('js/divStick.js')}}"></script>

	<!-- RESPONSIVE NAV !-->
	<script src="{{asset('/js/responsiveNav/modernizr.custom.js')}}"></script>
	<script src="{{asset('/js/responsiveNav/jquery.dlmenu.js')}}"></script>
	<script>
	    $(function() {
	        $( '#dl-menu' ).dlmenu();
	    });
	</script>

