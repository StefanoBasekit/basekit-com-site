<!doctype html>
<!--[if lt IE 7]>
	<html class="no-js ie6 oldie" lang="en">
<![endif]-->
<!--[if IE 7]>
	<html class="no-js ie7 oldie" lang="en">
<![endif]-->
<!--[if IE 8]>
	<html class="no-js ie8 oldie" lang="en">
<![endif]-->
<!--[if gt IE 8]><!-->
	<html class="no-js" lang="{{ page.seoLang }}">
<!--<![endif]-->
<head>
	<meta charset="utf-8" />
	<title>{{ page.title }}</title>
	<meta name="keywords" content="{{ page.keywords }}" />
	<meta name="description" content="{{ page.description }}" />
	<meta http-equiv="content-language" content="{{ page.seoLang }}" />

	{% if site.favicon %}
		<link rel="icon" href="{{ site.favicon['favicon'] }}" type="image/x-icon" />
		<link rel="SHORTCUT ICON" href="{{ site.favicon['thumbnail'] }}?v={{ "now"|date("U") }}" type="image/x-icon" />
	{% endif %}
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width,initial-scale=1">

	<link rel="stylesheet" type="text/css" href="{{asset('/css/rescols.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/fonts/font-awesome/css/font-awesome.min.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/js/previewer/stylesheet.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/js/slider/css/unoSlider.min.css')}}" />


	{% include basekit.headScript %}
</head>
<body class="{{ page.backgroundClass }}">

	{% block content %}
	{% endblock %}

	{% include basekit.bodyScript %}

	<!-- Validator --> 
	<script src="{{asset('js/validator/jquery.validator.js')}}"></script>
	<script type="text/javascript">
			$('#partner-signup').validate({
				rules: {
				    name: {
				      	required: true,
				      	rangelength: [1, 100]
				    },
				    telephone: {
				      	required: true,
				      	rangelength: [8, 100]
				    },
				    email: {
				      	required: true,
						email: true

				    },
				    password: {
				      	required: true,
				      	rangelength: [6, 100]
				    },
				    companyName: {
				      	required: false,
				      	maxlength: 100
				    },
				    website: {
				      	required: false,
				      	maxlength: 256
				    }
				},
				messages: {
					name: {
						required: "Please enter your full name",
						minlength: "Your full name must consist of at least 1 character",
						maxlength: "Your full name must be less than 100 characters"
					},
					telephone: {
						required: "Please enter your telephone number",
						minlength: "Your telephone must consist of at least 8 characters",
						maxlength: "Your telephone must be less than 100 characters"
					},
					email: {
						required: "Please enter a valid email address",
						minlength: "Your email must consist of at least 5 characters",
						maxlength: "Your full name must be less than 100 characters"
					},
					password: {
						required: "Please provide a password",
						minlength: "Your password must be at least 5 characters long",
						maxlength: "Your password must be less than 100 characters"
					},
					companyName: {
						maxlength: "Your company name must be less than 100 characters"
					},
					website: {
						maxlength: "Your website address must be less than 256 characters"
					}
				}
			});
	</script>

	<!-- ROUNDABOUT !-->
	<script type="text/javascript" src="templates/basekit-com-site/js/roundabout/jquery.roundabout.js"></script>
	<script type="text/javascript">
	   $(document).ready(function() {
	      $('ul.carousel').roundabout();
	   });
	</script>

	<script src="{{asset('js/previewer/preview.js')}}"></script>
	<script src="{{asset('js/divStick.js')}}"></script>

</body>
</html>
